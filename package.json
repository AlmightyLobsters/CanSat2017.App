{
  "name": "almighty_app",
  "version": "0.0.1",
  "description": "Data crounching application for the CanSat competition; based on nodejs, reactjs and electron",
  "main": "init.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "serve": "yarn run private:compile -- --source-maps true && run-p -r private:watch private:serve",
    "lint": "eslint --no-ignore scripts app test *.js",
    "pack": "run-s private:clean private:compile private:build:all",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "private:build:all": "build -mwl",
    "private:build:mac": "build --mac",
    "private:build:win": "build --win",
    "private:build:linux": "build --linux",
    "private:watch": "yarn run private:compile -- --source-maps true --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "node -e \"require('./app/styles/transpile')()\" && babel app/ --copy-files --out-dir build --ignore app/styles",
    "private:clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlmightyLobsters/LobsterPages.git"
  },
  "homepage": "http://almighty.lobsters.tech",
  "author": "James Bohacek <jamesbohacek@outlook.com>",
  "contributors": [
    "Vojtech Stepancik <vojtechstepancik@outlook.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "eslint-plugin-import": "^2.2.0",
    "file-url": "^2.0.2",
    "leaflet": "^1.0.3",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-c3js": "^0.1.16",
    "react-dom": "^15.4.2",
    "react-leaflet": "^1.1.4",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-csv": "^0.1.9",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "serialport": "^4.0.7",
    "sharp": "^0.17.2",
    "wallpaper": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.1.18",
    "browser-sync": "^2.9.3",
    "electron": "^1.4.15",
    "electron-builder": "^12.3.1",
    "electron-devtools-installer": "^2.1.0",
    "electron-rebuild": "^1.5.7",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "yarn-run-all": "^3.1.1"
  }
}
